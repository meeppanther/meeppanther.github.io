function getTwos(arrayItems) {
    var allItems = "";
    for (index = 0; index < arrayItems.length; index++) {
        if (index % 3 == 2) {
            allItems += "<div class='row'>";
        }
        allItems += "<div class='col-4 col-m-8 col-p-12' style='border-style: groove'><div id='div_iframe2'>"
        allItems += "<iframe id='fram' src='";
        allItems += arrayItems[index].src;
        allItems += "' title='YouTube video player' frameborder='0' allow='accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share' referrerpolicy='strict-origin-when-cross-origin' allowfullscreen></iframe></div>";// 2's is 11, 3's is 13 and tourney is 34
        allItems += "</div>";
        if (index % 3 == 2) {
            allItems += "</div>";
        }
    }
    allItems += "</div>";
    return allItems;
}
